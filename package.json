{
  "name": "@massds/mayflower-react",
  "description": "React versions of Mayflower design system UI components",
  "author": "Massachusetts Digital Services (MDS)",
  "version": "0.7.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "copy-data": "cp -R src/data lib && cp -R src/data es",
    "clean": "nwb clean-module",
    "start": "npm run storybook",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint --fix .; exit 0",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "prepublishOnly": "npm run build && npm run copy-data",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "pre-push": [
    "lint"
  ],
  "dependencies": {
    "@massds/mayflower": "^5.10.0",
    "pikaday": "^1.7.0",
    "prop-types": "^15.6.0",
    "react-select": "^1.2.1"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-info": "^3.3.10",
    "@storybook/addon-knobs": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addons": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "babel-core": "^6.26.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "nwb": "0.21.x",
    "pre-push": "^0.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "homepage": "",
  "license": "MIT",
  "repository": "https://github.com/massgov/mayflower-react",
  "keywords": [
    "react-component"
  ]
}

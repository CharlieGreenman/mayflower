<section class="ma__section-links">
  <!-- Banner is optional -->
  <div class="ma__section-links__banner">
    <img src="{{sectionLinks.image.src}}" alt="{{sectionLinks.image.alt}}" />
    <div class="ma__section-links__overlay"></div>
  </div>
  <div class="ma__section-links__content">
    <h3 class="ma__section-links__title">
      <a href="{{sectionLinks.title.href}}">{{sectionLinks.title.text}}</a>
    </h3>
    <ul class="ma__section-links__items">
      {% for list in sectionLinks.links %}
      <li class="ma__section-links__item">
        <a href="{{list.href}}" class="ma__section-links__link">{{list.text}}</a>
      </li>
      {% endfor %}
    </ul>
    <div class="ma__section-links__view-all">
      <a href="#" title="view all">{% include "atoms-svg-arrow" %}</a>
    </div>
  </div>
</section>
